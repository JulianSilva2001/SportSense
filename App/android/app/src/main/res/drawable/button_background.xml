<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient
                android:angle="270"
                android:startColor="#D5FFFFFF"
                android:endColor="#D5A8F8F6"
                android:type="linear"/>
            <corners android:radius="12dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:angle="270"
                android:startColor="#FFFFFF"
                android:endColor="#A8F8F6"
                android:type="linear"/>
            <corners android:radius="12dp" />
        </shape>
    </item>
</selector>
